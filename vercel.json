{
  "version": 2,
  "name": "soklin-frontend",
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/dist/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE": "$VITE_API_BASE",
    "VITE_WS_URL": "$VITE_WS_URL"
  }
}